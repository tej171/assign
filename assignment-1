var myName = "Teja";
console.log("myName is : " + myName);
console.log(`datatype of myName: ${typeof myName}`);

var Myage = 21;
console.log("My age is : " + Myage);
console.log(`datatype of Myage: ${typeof Myage}`);

let city = "Rajahmundry";
console.log("My city is : " + city);
console.log(`datatype of city: ${typeof city}`);

let student = true;
console.log("I am student : " + student);
console.log(`datatype of student: ${typeof student}`);

const Myname = null;
console.log(Myname);
console.log(`datatype of Myname: ${typeof Myname}`);

var college;
console.log(college);
console.log(`datatype of college: ${typeof college}`);

const pi = 3.14159;
console.log(pi)
console.log("Value of pi : " + pi);
console.log(`datatype of pi: ${typeof pi}`);

var greeting = "Hello, world!";
console.log(greeting);
console.log(`datatype of greeting: ${typeof greeting}`);

let isSunny = true;
console.log(isSunny);
console.log("whether it's sunny outside : " + isSunny);
console.log(`datatype of isSunny: ${typeof isSunny}`); 

const favouriteNumber = 9;
console.log("my favouriteNumber is : " + favouriteNumber);
console.log(`datatype of favouriteNumber: ${typeof favouriteNumber}`);


let mixedValue = ["hii", 12, true];
console.log(`datatype of mixedValue:${typeof mixedValue}`);
console.log(typeof mixedValue[0]);
console.log(typeof mixedValue[1]);
console.log(typeof mixedValue[2]);


let num1 = 20;
let num2 = 10;

let addResult = num1 + num2;
console.log(`Addition: ${addResult}`); 

let subResult = num1 - num2;
console.log(`Subtraction:${subResult}`); 

let mulResult = num1 * num2;
console.log(`Multiplication:${mulResult}`); 

let divResult = num1 / num2;
console.log(`Division:${divResult}`); 

let modulusResult = num1 % num2;
console.log(`Modulus:${modulusResult}`); 

let expoResult = num1 ** num2; 
console.log(`Exponential:${expoResult}`); 


const x = 5;
const y = "5";
console.log(x==y);

const year = 2024;
const Tejage = year - 2003;
const Reshmaage = year - 2004;
console.log(Tejage != Reshmaage);
console.log(Tejage > Reshmaage);
console.log(Tejage < Reshmaage);
console.log(Tejage >= 21);
console.log(Reshmaage <= 19);


const backlogs = false;
const Eligiblefortcsplacement = true;
console.log(backlogs && Eligiblefortcsplacement);
console.log(backlogs || Eligiblefortcsplacement);
console.log(!(backlogs || Eligiblefortcsplacement));

let Age = 21;
Age >= 18 ? console.log("You are eligible to vote") : console.log("You are not eligible to vote")

